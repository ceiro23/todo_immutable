{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "target": "es6",
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["es2015", "dom", "dom.iterable"],
    "outDir": "dist",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
